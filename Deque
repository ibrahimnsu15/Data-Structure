#include <iostream>
#include <deque>
using namespace std;

int main()
{
    deque<int> dq;
    
    /// 3 1 2 4
    // push_back(x) , push_front(x);
    dq.push_back(2);
    dq.push_front(1);
    dq.push_back(4);
    dq.push_front(3);

    /// front() only
    
    while(!dq.empty()){
        cout<<dq.front()<<endl;
        dq.pop_front();
    }

    return 0;
}
