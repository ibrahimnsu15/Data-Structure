#include <iostream>
#include <deque>
using namespace std;


int main()
{
    deque<int> dq;

    /// push_back(x) , push_front(x)

    dq.push_back(2);
    dq.push_front(1);
    dq.push_back(4);
    dq.push_front(3);

    /// list 3 1 2 4
    
    
    /// pop_front() pop from front
    /// pop_back() pop from back
    /// front() gives front element
    /// back() gives back element
    
    
    while(!dq.empty()){
        cout<<dq.front()<<" "<<dq.back()<<endl;
        dq.pop_back();
        dq.pop_front();
    }
    
    /// 3 4
    /// 1 2

    return 0;
}
